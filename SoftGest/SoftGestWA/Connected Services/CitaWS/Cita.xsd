  </xs:complexType>
  <xs:complexType name="obtenerPorIdCita">
    <xs:sequence>
      <xs:element minOccurs="0" name="idCita" type="xs:int" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="obtenerPorIdCitaResponse">
    <xs:sequence>
      <xs:element minOccurs="0" name="return" type="tns:citaDTO" />
